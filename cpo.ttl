cco:BeliefDisposition rdf:type owl:Class ;
                      rdfs:subClassOf cco:DispositionToCognitivelyRepresent ;
                      cco:definition "A Disposition to Cognitively Represent that is realized in the formation of a Representation that is Believed." ;
