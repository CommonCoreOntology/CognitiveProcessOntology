                                  rdfs:subClassOf cco:CognitiveUtilizationDisposition ;
                                  cco:definition "A Cognitive Utilization Disposition realized in a Cognitive Process of Inferring." ;
                                  cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                                  rdfs:label "CognitiveInferenceDisposition" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CognitiveProcess
cco:CognitiveProcess rdf:type owl:Class ;
                     rdfs:subClassOf cco:RepresentationalMentalProcess ;
                     cco:definition "A mental process that brings into being, sustains, or modifies a cognitive representation." ;
                     cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                     rdfs:label "CognitiveProcess" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CognitiveProcessOfAssociation
cco:CognitiveProcessOfAssociation rdf:type owl:Class ;
                                  rdfs:subClassOf cco:CognitiveUtilizationProcess ;
                                  cco:definition "A Cognitive Process that just is the taking of some Cognitive Representation(s) as input and, based on some non-similarity and some non-evidential relation(s) between the input(s) and some representation(s), outputting some Cognitive Representation." ;
                                  cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                                  rdfs:label "CognitiveProcessOfAssociation" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CognitiveProcessOfComparing
cco:CognitiveProcessOfComparing rdf:type owl:Class ;
                                rdfs:subClassOf cco:CognitiveUtilizationProcess ;
                                cco:definition "A Cognitive Process that just is the taking of some Cognitive Representations as input and, based on the similarity relations represented by the inputs, outputting some Cognitive Representation." ;
                                cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                                rdfs:label "CognitiveProcessOfComparing" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CognitiveProcessOfInferring
cco:CognitiveProcessOfInferring rdf:type owl:Class ;
                                rdfs:subClassOf cco:CognitiveUtilizationProcess ;
                                cco:definition "A Cognitive Process that just is the taking of some Cognitive Representation(s) as input and, based on the semantic or syntactic relations of the input(s), outputting some Cognitive Representation." ;
                                cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                                rdfs:label "CognitiveProcessOfInferring" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/CognitiveRepresentation
cco:CognitiveRepresentation rdf:type owl:Class ;
                            rdfs:subClassOf cco:MentalRepresentation ;
                            cco:elucidation "A Mental Representation that has a mind-to-world direction of fit." ;
                            cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                            rdfs:label "CognitiveRepresentation" .


