###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfRequestingInformationCollection
cco:ActOfRequestingInformationCollection rdf:type owl:Class ;
                                         rdfs:subClassOf cco:ActOfRequestingInformation ;
                                         cco:definition "An act of directive communication that prescribes an act of information collection." ;
                                         cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                                         rdfs:label "ActOfRequestingInformationCollection" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfRequestingInformationRetrieval
cco:ActOfRequestingInformationRetrieval rdf:type owl:Class ;
                                        rdfs:subClassOf cco:ActOfRequestingInformation ;
                                        cco:definition "An act of directive communication that prescribes an act of information retrieval." ;
                                        cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                                        rdfs:label "ActOfRequestingInformationRetrieval" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfValidation
cco:ActOfValidation rdf:type owl:Class ;
                    rdfs:subClassOf cco:ActOfIntelligenceGathering ;
                    cco:definition "A planned process that confirms that an intelligence collection or production requirement is sufficiently important to justify the dedication of intelligence resources, does not duplicate an existing requirement, and has not been previously satisfied." ;
                    cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                    rdfs:comment "These need to be evaluated and moved. #issue -dgl" ;
                    rdfs:label "ActOfValidation" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActionSpecification
cco:ActionSpecification rdf:type owl:Class ;
                        rdfs:subClassOf cco:DirectiveInformationContentEntity ;
                        cco:definition "a directive information entity that prescribes an action." ;
                        cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                        rdfs:label "ActionSpecification" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/AffectiveRepresentation
cco:AffectiveRepresentation rdf:type owl:Class ;
                            rdfs:subClassOf cco:MentalRepresentation ;
                            cco:elucidation "A mental representation that has no direction of fit but is purely, and subjectively, evaluative." ;
                            cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                            rdfs:label "AffectiveRepresentation" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/AnalogyComparing
cco:AnalogyComparing rdf:type owl:Class ;
                     rdfs:subClassOf cco:IsomorphicComparing ;
                     cco:definition "An IsomorphicComparing in which an AnalogyReprosentation and some other CognitiveRepresentation participate and are compared for the sake of reasoning and forming new representations that are believed." ;
                     cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                     rdfs:label "AnalogyComparing" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/AnatomicalStructure
cco:AnatomicalStructure rdf:type owl:Class ;
                        rdfs:subClassOf obo:BFO_0000030 ;
                        cco:definition "An object which is generated by coordinated expression of the organism's own genes that guide its morphogenesis; has inherent 3D shape; its parts are connected and spatially related to one another in patterns determined by coordinated gene expression. Examples: embryo, mesoderm, heart, right ventricle, mitral valve, myocardium, endothelium, lymphocyte, fibroblast, thorax, cardiovascular system, hemoglobin, T cell receptor." ;
                        cco:definition_source "https://bioportal.bioontology.org/ontologies/FMA/?p=classes&conceptid=http%3A%2F%2Fpurl.org%2Fsig%2Font%2Ffma%2Ffma62955" ;
                        cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                        rdfs:comment "Definition adapted form the Foundational Model of Anatomy (FMA). FMA has a number of parent classes that are not included here. This is an unresolved issue. #issue -dgl" ;
                        rdfs:label "AnatomicalStructure" .


