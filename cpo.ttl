#    Classes
#################################################################

###  http://www.ontologyrepository.com/CommonCoreOntologies/AcceptanceDecision
cco:AcceptanceDecision rdf:type owl:Class ;
                       rdfs:subClassOf cco:Decision ;
                       cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                       rdfs:label "AcceptanceDecision" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/AcceptanceDecisionSpecification
cco:AcceptanceDecisionSpecification rdf:type owl:Class ;
                                    rdfs:subClassOf cco:DecisionSpecification ;
                                    cco:definition "A decision specificaiton that specifies that some proposition in a binary decision is to be carried out" ;
                                    cco:definition_source "https://raw.githubusercontent.com/UMassCenterforeDesign/decision-making/master/decision_core.owl" ;
                                    cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                                    rdfs:label "AcceptanceDecisionSpecification" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfAttending
cco:ActOfAttending rdf:type owl:Class ;
                   rdfs:subClassOf cco:PerceivingProcess ;
                   cco:definition "The process whereby relevant aspects of our mental experience are focused on specific portions of reality." ;
                   cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                   rdfs:label "ActOfAttending" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfCharacterization
cco:ActOfCharacterization rdf:type owl:Class ;
                          rdfs:subClassOf cco:IntentionalAct ;
                          cco:definition "The act of describing distinctive characteristics of some portion of reality." ;
                          cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                          rdfs:label "ActOfCharacterization" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfCommunication
cco:ActOfCommunication rdf:type owl:Class ;
                       cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                       rdfs:label "ActOfCommunication" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfCritique
cco:ActOfCritique rdf:type owl:Class ;
                  rdfs:subClassOf cco:ActOfEvaluation ;
                  cco:definition "a detalied assessment of some portion of reality that was intentionally created." ;
                  cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                  rdfs:label "ActOfCritique" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfDeceptiveCommunication
cco:ActOfDeceptiveCommunication rdf:type owl:Class ;
                                rdfs:subClassOf cco:ActOfCommunication ;
                                cco:definition "An Act of Communication intended to mislead the audience by distortion or falsification of evidence and induce a reaction that is prejudicial to the interests of the audience." ;
                                cco:definition_source "https://raw.githubusercontent.com/UMassCenterforeDesign/decision-making/master/decision_core.owl" ;
                                cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                                rdfs:label "Act of Deceptive Communication" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfEvaluation
cco:ActOfEvaluation rdf:type owl:Class ;
                    rdfs:subClassOf cco:ActOfAttending ;
                    cco:definition "A cognitive process that systematically appraises the significance or value of some portion of reality according to specified objectives and criteria." ;
                    cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                    rdfs:label "ActOfEvaluation" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfInformationCollection
cco:ActOfInformationCollection rdf:type owl:Class ;
                               rdfs:subClassOf cco:IntentionalAct ;
                               cco:definition "A process in response to an act of requesting information collection that is the acquiring of information that is not currently had by the agent's organization and the delivering of that informaiton to the requesting agent." ;
                               cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                               rdfs:label "ActOfInformationCollection" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfInformationRetrieval
cco:ActOfInformationRetrieval rdf:type owl:Class ;
                              rdfs:subClassOf cco:IntentionalAct ;
                              cco:definition "A process in response to an act of requesting information retrieval that is the acquiring of information, had by the agent's organization, and devivering it to the requesting agent." ;
                              cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                              rdfs:label "ActOfInformationRetrieval" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfIntelligenceGathering
cco:ActOfIntelligenceGathering rdf:type owl:Class ;
                               cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                               rdfs:label "ActOfIntelligenceGathering" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfInterpretation
cco:ActOfInterpretation rdf:type owl:Class ;
                        rdfs:subClassOf cco:IntentionalAct ;
                        cco:definition "An intentional act of elucidation, explication, or explanation of the meaning or significance of some portion of reality." ;
                        cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                        rdfs:label "ActOfInterpretation" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfInvestigation
cco:ActOfInvestigation rdf:type owl:Class ;
                       rdfs:subClassOf cco:CognitiveProcess ;
                       cco:definition "A cognitive process whose agent intends to establish or confirm that some portion of reality exists or does not exist." ;
                       cco:definition_source "PAPER: WARRANTED DIAGNOSIS" ;
                       cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                       rdfs:label "ActOfInvestigation" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfJudging
cco:ActOfJudging rdf:type owl:Class ;
                 rdfs:subClassOf cco:CognitiveProcess ;
                 cco:definition "Judging is the evaluation of evidence to make a decision or form a belief." ;
                 cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                 rdfs:label "ActOfJudging" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfMonitoring
cco:ActOfMonitoring rdf:type owl:Class ;
                    rdfs:subClassOf cco:ActOfInvestigation ;
                    cco:definition "An act of investigation wherby an agent waits for a change in, and observes, a circumstance with the intention of confirming or disconfirming the existence of some portion of reality." ;
                    cco:definition_source "Google Dictionary" ;
                    cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                    rdfs:comment "This needs work to distinguish it from, for example, the act of answering the door." ,
                                 "This was originally a subclass of CCO:Act of Observation, however seems better suited to be a subclass of an Act of Investigation." ;
                    rdfs:label "ActOfMonitoring" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfQuestioning
cco:ActOfQuestioning rdf:type owl:Class ;
                     rdfs:subClassOf cco:CognitiveProcess ;
                     cco:definition "A cogntiive process that is an act of requesting information." ;
                     cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                     rdfs:label "ActOfQuestioning" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfRequesting
cco:ActOfRequesting rdf:type owl:Class ;
                    cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
                    rdfs:label "ActOfRequesting" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/ActOfRequestingAnInvestigation
cco:ActOfRequestingAnInvestigation rdf:type owl:Class ;
                                   rdfs:subClassOf cco:ActOfInformationCollection ;
                                   cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CognitiveProcessOntology" ;
